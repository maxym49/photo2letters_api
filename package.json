    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^9.2.5",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.3",
    "morgan": "~1.9.1",
    "node-tesseract-ocr": "^0.2.0",
